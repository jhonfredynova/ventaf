{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"next/core-web-vitals",
		"airbnb",
		"plugin:jest/recommended",
		"plugin:jsx-a11y/recommended",
		"plugin:react/recommended",
		"prettier"
	],
	"rules": {
		"react/jsx-props-no-spreading": "off",
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"react/prop-types": "off"
	}
}
